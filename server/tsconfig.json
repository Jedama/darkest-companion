{
    "compilerOptions": {
      "target": "ES2021",
      "module": "ESNext",
      "moduleResolution": "bundler",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "skipLibCheck": true,
      "outDir": "dist",
      "types": ["node", "express"],
      "noImplicitAny": true
    },
    "include": ["./**/*.ts"],
    "exclude": ["node_modules", "dist"]
  }